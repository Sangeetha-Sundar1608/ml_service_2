# ============================================================================
# REQUIRED - EDIT THESE
# ============================================================================

# Database - Change password!
DATABASE_URL=postgresql://postgres@localhost:5433/postgres

# Redis - Usually default is fine
REDIS_URL=redis://localhost:6379

# LLM Providers - Add what you have
# If you have vLLM server:
VLLM_SERVICE_URL=http://YOUR_SERVER_IP:8000

# If you have Ollama locally:
OLLAMA_URL=http://localhost:11434

# If you have OpenAI API key:
OPENAI_API_KEY=sk-proj-xxxxxxxxxxxxx

# If you have Anthropic API key:
# ANTHROPIC_API_KEY=sk-ant-xxxxxxxxxxxxx

# ============================================================================
# OPTIONAL - Can leave as defaults
# ============================================================================
ENVIRONMENT=development
DEBUG=True
ML_SERVICE_PORT=8001
LOG_LEVEL=INFO